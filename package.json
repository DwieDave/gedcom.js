{
  "name": "gedcom.js",
  "version": "0.0.1",
  "description": "Client for the use of the Gedcom File Format v7 with node.js",
  "main": "index.js",
  "scripts": {
    "start": "node ./lib/index.js",
    "generateGedcomParser": "npx nearleyc ./lib/grammar/Gedcom.ne -o ./lib/grammar/parser/GedcomParser.js",
    "generateAndStart": "npm run generateGedcomParser && npm run start",
    "generateSubGrammars": "node ./lib/helpers/generateSubGrammars.js",
    "test": "mocha ./test/gedcomMochaTest.mjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DwieDave/gedcom.js.git"
  },
  "keywords": [
    "gedcom",
    "v7",
    "family",
    "family",
    "tree",
    "family",
    "search",
    "pedigree",
    "genealogy"
  ],
  "author": "David Gruber & Marius MÃ¼ller",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DwieDave/gedcom.js/issues"
  },
  "homepage": "https://github.com/DwieDave/gedcom.js#readme",
  "devDependencies": {
    "mocha": "^10.0.0"
  },
  "dependencies": {
    "moo": "^0.5.2",
    "moo-ignore": "^2.5.1",
    "nearley": "^2.20.1"
  }
}
